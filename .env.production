VITE_OPENAI_API=your-openai-api-key-here
VITE_OPENAI_MODEL=gpt-3.5-turbo

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Munus API
VERSION=1.0.0
DESCRIPTION=AI-Powered Job Portal & Resume Builder API

# Security
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Database
DATABASE_URL=your-database-url-here
ASYNC_DATABASE_URL=your-async-database-url-here

# CORS
BACKEND_CORS_ORIGINS=["https://www.gomunus.com","https://gomunus.com","https://api.gomunus.com"]

# Email Configuration
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-email-password
MAIL_FROM=your-email@gmail.com
MAIL_PORT=587
MAIL_SERVER=smtp.gmail.com
MAIL_FROM_NAME=Munus

# File Storage
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_BUCKET_NAME=your-bucket-name
AWS_REGION=us-east-1

# Redis
REDIS_URL=your-redis-url

# Environment
ENVIRONMENT=production
DEBUG=false

# MongoDB
MONGODB_URI=your-mongodb-uri
MONGODB_DB_NAME=jobify
VITE_API_BASE_URL=https://api.gomunus.com

# Vercel Analytics
VERCEL_ANALYTICS_ID=your-vercel-analytics-id

# Supabase (if needed)
VITE_SUPABASE_URL=your-supabase-url
