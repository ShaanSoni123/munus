{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/app/main.py"
    },
    {
      "src": "/health",
      "dest": "/backend/app/main.py"
    },
    {
      "src": "/docs",
      "dest": "/backend/app/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "/backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "MONGODB_DB_NAME": "munus",
    "SECRET_KEY": "@secret_key",
    "ALGORITHM": "HS256",
    "ACCESS_TOKEN_EXPIRE_MINUTES": "30",
    "REFRESH_TOKEN_EXPIRE_DAYS": "7",
    "BACKEND_CORS_ORIGINS": "[\"https://www.gomunus.com\",\"https://gomunus.com\",\"https://vercel.app\"]",
    "ENVIRONMENT": "production",
    "DEBUG": "false",
    "API_V1_STR": "/api/v1",
    "PROJECT_NAME": "Munus API",
    "VERSION": "1.0.0"
  }
} 